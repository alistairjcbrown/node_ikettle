{
    "curly":      true,
    "devel":      false,
    "eqeqeq":     true,
    "eqnull":     true,
    "expr":       true,
    "immed":      true,
    "indent":     4,
    "latedef":    true,
    "maxdepth":   3,
    "maxlen":     140,
    "maxparams":  10,
    "newcap":     true,
    "noarg":      true,
    "noempty":    true,
    "quotmark":   "double",
    "strict":     true,
    "trailing":   true,
    "undef":      true,
    "unused":     true,
    "globals": {
        "define":        true,
        "env":           true,
        "expect":        true,
        "injector":      true,
        "mocha":         true,
        "module":        true,
        "require":       true,
        "setup":         true,
        "suite":         true,
        "suiteSetup":    true,
        "suiteTeardown": true,
        "teardown":      true,
        "test":          true,
        "Buffer":        true,

        "process":       true,
        "console":       true,
        "global":        true
    }
}
